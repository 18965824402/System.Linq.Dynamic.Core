{
    "version": "1.0.0.0-*",
    "authors": [ "Stef Heyenrath" ],
    "dependencies": {
        "System.Linq.Dynamic.Core": "1.0.1-*"
    },

    "compilationOptions": {
        "emitEntryPoint": true
    },

    "compile": [
        "../../test/System.Linq.Dynamic.Core.Tests/Helpers/*.cs",
        "../../test/System.Linq.Dynamic.Core.Tests/Helpers/Models/*.cs",
        "../../test/System.Linq.Dynamic.Core.Tests/Helpers/Entities/*.cs",
        "../../test/System.Linq.Dynamic.Core.Tests/Logging/*.cs"
    ],

    "commands": {
        "ConsoleTestApp": "run"
    },

    "frameworks": {
        "dnx452": {
            "dependencies": {
                "EntityFramework.Core": "7.0.0-rc1-*",
                "EntityFramework.MicrosoftSqlServer": "7.0.0-rc1-*",
                "EntityFramework.Sqlite": "7.0.0-rc1-*",
                "EntityFramework": "6.1.3",

                "System.Linq.Parallel": "4.0.1-beta-*",
                "System.Threading.Tasks": "4.0.11-beta-*"
            },
            "frameworkAssemblies": {
                "System.Data.Entity": ""
            }
        }
    }
}